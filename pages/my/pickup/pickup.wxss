page{
  background-color: #f2f2f2;
  padding-top: 10rpx;
}
.btn-pickup{
  height: 82rpx;
  line-height: 82rpx;
  margin-bottom: 6rpx;
  background-color: #fff;
  padding: 0 20rpx;
  border-top: solid 1rpx #e4e4e4;
  font-size: 30rpx;
  color: #3f3f3f;
}
.list{
  background-color: #fff;
}
.item{
  display: flex;
  flex-flow: row;
  border-bottom: solid 1rpx #eee;
  padding: 20rpx;
  position: relative;
  font-size: 26rpx;
  color: #656565;
}
.item text{
  flex: 1;
}
.footer{
  background:#f9f9f9;
  color:#a9b7b7;
  font-size: 24rpx;
  text-align: center;
  position: fixed;
  bottom:0;
  width: 100%;
  padding: 10rpx 0 5rpx;
}
.footer navigator{
  display: inline-block;
  width: 25%;
}
.footer navigator image{

  width: 50rpx;
  height: 50rpx;
  display: block;
  margin: 0 auto 8rpx;
}
.footer .cur{
  color: #1b1b1b
}
.blank{
  height: 97rpx
}