/* pages/my/doctor/visitTime/visitTime.wxss */
page{
  background-color: #f6f6f6;
}
.tableItem{
  background-color: #fff;
  display: flex;
  padding:20rpx 40rpx 20rpx 20rpx;
  text-align:center;
  border-bottom: solid 2rpx #eee;
  font-size: 30rpx;
  color: #333;
}
.tableItem .item{
  height: 60rpx;
  line-height: 60rpx;
  flex: 1;
}
.tableItem .item image{
  width: 26rpx;
  height: 26rpx;
}
checkbox{
   opacity: 0; 
   width: 0;
}
.checkbox{
  font-family: Arial, Helvetica, sans-serif;
  font-size: 30rpx;
  font-weight: bold;
  color: #999;
}
.btn-submit{
  position: fixed;
  bottom: 0;
  left: 0;
  width:100%;
  height:90rpx;
  font-size:32rpx;
  text-align:center;
  line-height:90rpx;
  background-color:#b6002d;
  color:#fff;
}