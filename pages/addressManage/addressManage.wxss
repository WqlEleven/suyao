page{
  background-color: #f1f1f1;
}
.administration {
  width: 100%;
  position: relative;
  padding-bottom:100rpx;
}
.title{
  background-color: #c40000;
  color: #fff;
  padding: 0 37rpx;
  height: 76rpx;
  line-height: 76rpx;
  font-size: 30rpx;
}
.title image{
  width: 19rpx;
  height: 29rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 19rpx;
}
.administration_con {
  background-color: #fff;
  border-bottom: 1rpx solid #d5d5d5;
  padding: 20rpx 22rpx;
  margin: 5rpx 0;
}
.administration_con .list1 {
  color: #373737;
  font-size: 26rpx;
  display: flex;
  padding: 5rpx 0;
}

.administration_con .list1 view:first-child {
  width: 150rpx;
}

.administration_con .list2 {
  color: #373737;
  font-size: 26rpx;
  padding: 5rpx 0;
}

.administration_con .list3 {
  overflow: hidden;
  padding: 20rpx 0 10rpx;
}

.administration_con .list3 .float {
  float: right;
  display: flex;
  font-size: 28rpx;
  color: #979797;
  line-height: 30rpx;
  margin-left: 40rpx;
}

.administration_con .list3 .float image {
  width: 30rpx;
  margin-right: 5rpx;
}

.administration_con .list3 .float icon {
  margin-right: 5rpx;
  margin-top: 2rpx;
}

.label {
  overflow: hidden;
  display: flex;
  line-height: 30rpx;
  height: 30rpx;
}

.label icon {
  float: left;
}

.label .last {
  float: left;
}
.save-btn {
  width: 100%;
  height: 90rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 90rpx;
  background-color: #b6002d;
  color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  border-radius: 0;
}
.save-btn image{
  width: 34rpx;
  vertical-align:middle;
}

.save-btn:active {
  background-color: #b6002d;
}
.noAddmanegr{
    text-align: center;
    padding:20rpx;
    font-size: 30rpx;
    color: #490012;
}
